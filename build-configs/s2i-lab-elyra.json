{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "s2i-lab-elyra",
                "labels": {
                    "build": "s2i-lab-elyra",
                    "opendatahub.io/notebook-image": "true"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                },
                "tags": [
                    {
                        "annotations": {
                            "openshift.io/imported-from": "quay.io/thoth-station/s2i-lab-elyra"
                        },
                        "from": {
                            "kind": "DockerImage",
                            "name": "quay.io/thoth-station/s2i-lab-elyra:v0.0.6"
                        },
                        "importPolicy": {},
                        "name": "v0.0.6",
                        "referencePolicy": {
                            "type": "Source"
                        }
                    },
                    {
                        "annotations": {
                            "openshift.io/imported-from": "quay.io/thoth-station/s2i-lab-elyra"
                        },
                        "from": {
                            "kind": "DockerImage",
                            "name": "quay.io/thoth-station/s2i-lab-elyra:v0.0.7"
                        },
                        "importPolicy": {},
                        "name": "v0.0.7",
                        "referencePolicy": {
                            "type": "Source"
                        }
                    }
                ]
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "s2i-lab-elyra",
                "labels": {
                    "build": "s2i-lab-elyra"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/kkasravi/s2i-lab-elyra",
                        "ref": "kkasravi/v0.0.8"
                    }
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "s2i-minimal-py38-notebook:v0.0.14"
                        },
                        "env": [
                          {
                            "name": "JUPYTER_ENABLE_LAB",
                            "value": "true"
                          },
                          {
                            "name": "ENABLE_MICROPIPENV",
                            "value": "1"
                          },
                          {
                            "name": "CRIO_RUNTIME",
                            "value": "true"
                          },
                          {
                            "name": "THAMOS_RUNTIME_ENVIRONMENT",
                            "value": "python38"
                          },
                          {
                            "name": "THOTH_ADVISE",
                            "value": "0"
                          },
                          {
                            "name": "THOTH_ERROR_FALLBACK",
                            "value": "1"
                          },
                          {
                            "name": "THOTH_DRY_RUN",
                            "value": "1"
                          },
                          {
                            "name": "THAMOS_DEBUG",
                            "value": "0"
                          },
                          {
                            "name": "THAMOS_VERBOSE",
                            "value": "1"
                          },
                          {
                            "name": "THOTH_PROVENANCE_CHECK",
                            "value": "0"
                          },
                          {
                            "name": "GIT_REPO_NAME",
                            "value": "s2i-lab-elyra"
                          }
                        ]
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-lab-elyra:v0.0.8"
                    }
                }
            }
        }
    ]
}
