{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "kam-aikit-notebook",
                "labels": {
                    "build": "kam-aikit-notebook",
                    "opendatahub.io/notebook-image": "true"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "kam-aikit-notebook",
                "labels": {
                    "build": "kam-aikit-notebook"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/kkasravi/s2i-minimal-notebook",
                        "ref": "s2i-intel-aikit-miniconda"
                    }
                },
                "strategy": {
                    "type": "Docker",
                    "dockerStrategy": {
                        "dockerfilePath": "Dockerfile.intel-aikit"
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "kam-aikit-notebook:v0.0.1"
                    }
                }
            }
        }
    ]
}
