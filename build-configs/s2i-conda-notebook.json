{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "s2i-conda-notebook",
                "labels": {
                    "build": "s2i-conda-notebook",
                    "opendatahub.io/notebook-image": "true"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "s2i-conda-notebook",
                "labels": {
                    "build": "s2i-conda-notebook"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/preethivenkatesh/s2i-minimal-notebook",
                        "ref": "master"
                    }
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "s2i-conda-py37:latest"
                        },
                        "env": [
                          {
                            "name": "ENABLE_PIPENV",
                            "value": "1"
                          },
                          {
                            "name": "THOTH_ADVISE",
                            "value": "0"
                          },
                          {
                            "name": "THOTH_DRY_RUN",
                            "value": "0"
                          },
                          {
                            "name": "THOTH_PROVENANCE_CHECK",
                            "value": "0"
                          }
                        ]
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-conda-notebook:v0.0.1"
                    }
                }
            }
        }
    ]
}
